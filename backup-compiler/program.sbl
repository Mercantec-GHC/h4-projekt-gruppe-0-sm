
#[c_function("notice")]
fn notice() {}

#[c_function("print_int")]
fn print_int(value) {}

fn inner(value) {
    print_int(value);
    return 0;    
}

fn main() {
    let i = 0;
    loop {
        if 10 < i + 1 {
            break;
        }
        let a = 4;
        inner(a + 2);

        i = i + 1;
    }
    return i;
}

